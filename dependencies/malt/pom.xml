<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.maltparser</groupId>
	<artifactId>malt</artifactId>
	<packaging>pom</packaging>
	<version>1.4.1</version>
	<name>MaltParser</name>
	<parent>
		<artifactId>cleartk-dependencies</artifactId>
		<groupId>org.cleartk</groupId>
		<version>0.0.1</version>
		<relativePath>..</relativePath>
	</parent>
	<repositories>
		<repository>
			<id>umass-nexus-thirdparty</id>
			<name>UMass Nexus third party jars</name>
			<url>http://iesl.cs.umass.edu:8081/nexus/content/repositories/thirdparty</url>
		</repository>
	</repositories>
	<dependencies>
		<dependency>
			<groupId>javax.xml.bind</groupId>
			<artifactId>jsr173_api</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>libsvm</groupId>
			<artifactId>libsvm</artifactId>
			<version>2.89</version>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.16</version>
		</dependency>
		<dependency>
			<groupId>stax</groupId>
			<artifactId>stax</artifactId>
			<version>1.2.0</version>
		</dependency>
		<dependency>
			<groupId>liblinear</groupId>
			<artifactId>liblinear</artifactId>
			<version>1.51</version>
		</dependency>
	</dependencies>
	<!-- Attach the jar file to the artifact (that would just be a pom otherwise) -->
	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>
</project>