<version>1.0</version>
<vendor></vendor>
<configurationParameters searchStrategy="language_fallback">
<configurationParameter>
<name>buildModelFile</name>
<type>String</type>
<multiValued>false</multiValued>
<mandatory>true</mandatory>
</configurationParameter>
<configurationParameter>
<name>checkModelFile</name>
<type>String</type>
<multiValued>false</multiValued>
<mandatory>true</mandatory>
</configurationParameter>
<configurationParameter>
<name>chunkModelFile</name>
<type>String</type>
<multiValued>false</multiValued>
<mandatory>true</mandatory>
</configurationParameter>
<configurationParameter>
<name>headRulesFile</name>
<type>String</type>
<multiValued>false</multiValued>
<mandatory>true</mandatory>
</configurationParameter>
<configurationParameter>
<name>beamSize</name>
<type>Integer</type>
<multiValued>false</multiValued>
<mandatory>false</mandatory>
</configurationParameter>
<configurationParameter>
<name>advancePercentage</name>
<type>Float</type>
<multiValued>false</multiValued>
<mandatory>false</mandatory>
</configurationParameter>
</configurationParameters>
<configurationParameterSettings>
<nameValuePair>
<name>buildModelFile</name>
<value>
<string>resources/models/OpenNLP.Parser.English.Build.bin.gz</string>
</value>
</nameValuePair>
<nameValuePair>
<name>chunkModelFile</name>
<value>
<string>resources/models/OpenNLP.Chunker.English.bin.gz</string>
</value>
</nameValuePair>
<nameValuePair>
<name>headRulesFile</name>
<value>
<string>resources/models/OpenNLP.HeadRules.txt</string>
</value>
</nameValuePair>
<nameValuePair>
<name>checkModelFile</name>
<value>
<string>resources/models/OpenNLP.Parser.English.Check.bin.gz</string>
</value>
</nameValuePair>
</configurationParameterSettings>
<typeSystemDescription>
<imports>
<import location="../../TypeSystem.xml"/>
</imports>
</typeSystemDescription>
<typePriorities/>
<capabilities>
<capability>
<inputs>
<type>org.cleartk.type.Token</type>
<type>org.cleartk.type.Sentence</type>
<feature>org.cleartk.type.Token:pos</feature>
<feature>org.cleartk.type.Token:end</feature>
<feature>org.cleartk.type.Token:begin</feature>
<feature>org.cleartk.type.Sentence:tokens</feature>
<feature>org.cleartk.type.Sentence:end</feature>
<feature>org.cleartk.type.Sentence:begin</feature>
</inputs>
<outputs>
<type allAnnotatorFeatures="true">org.cleartk.type.bank.TreebankSplitArgument</type>
<type allAnnotatorFeatures="true">org.cleartk.type.bank.TreebankObject</type>
<type allAnnotatorFeatures="true">org.cleartk.syntax.treebank.type.TreebankNode</type>
<type allAnnotatorFeatures="true">org.cleartk.type.bank.TreebankCoreference</type>
<type allAnnotatorFeatures="true">org.cleartk.syntax.treebank.type.TopTreebankNode</type>
<feature>org.cleartk.type.Sentence:constituentParse</feature>
</outputs>
<languagesSupported>
<language>en-US</language>
</languagesSupported>
</capability>
</capabilities>
<operationalProperties>
<modifiesCas>true</modifiesCas>
<multipleDeploymentAllowed>true</multipleDeploymentAllowed>
<outputsNewCASes>false</outputsNewCASes>
</operationalProperties>
</analysisEngineMetaData>
<resourceManagerConfiguration/>
</analysisEngineDescription>
