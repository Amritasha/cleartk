<?xml version="1.0" encoding="UTF-8"?>
<cpeDescription xmlns="http://uima.apache.org/resourceSpecifier">
<collectionReader>
<collectionIterator>
<descriptor>
<import location="../util/PlainTextCollectionReader.xml"/>
</descriptor>
<configurationParameterSettings>
<nameValuePair>
<name>FileOrDirectory</name>
<value>
<string>data/timebank/wsj</string>
</value>
</nameValuePair>
<nameValuePair>
<name>ViewName</name>
<value>
<string>TimeMLView</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</collectionIterator>
</collectionReader>
<casProcessors casPoolSize="3" processingUnitThreadCount="1">
<casProcessor deployment="integrated" name="TimeMLGoldAnnotator">
<descriptor>
<import location="../corpus/timeml/TimeMLGoldAnnotator.xml"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>LoadTlinks</name>
<value>
<boolean>false</boolean>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
<casProcessor deployment="integrated" name="StandoffPlainTextTLINKGoldAnnotator">
<descriptor>
<import location="../corpus/timeml/PlainTextTLINKGoldAnnotator.xml"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>TlinkFileUrl</name>
<value>
<string>http://verbs.colorado.edu/~bethard/data/timebank-verb-clause.txt</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
<casProcessor deployment="integrated" name="TreebankAligningAnnotator">
<descriptor>
<import location="../corpus/timeml/TreebankAligningAnnotator.xml"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>TreebankDirectory</name>
<value>
<string>data/treebank/wsj</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
<casProcessor deployment="integrated" name="SnowballStemmer">
<descriptor>
<import location="../token/snowball/SnowballStemmer.xml"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
</casProcessor>
<casProcessor deployment="integrated" name="VerbClauseTemporalDataWriter">
<descriptor>
<import location="VerbClauseTemporalDataWriter.xml"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>OutputDirectory</name>
<value>
<string>test/data/temporal</string>
</value>
</nameValuePair>
<nameValuePair>
<name>FeaturesEncoderFactoryClass</name>
<value>
<string>org.cleartk.classifier.encoder.factory.FileSystemEncoderFactory</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
</casProcessors>
<cpeConfig>
<numToProcess>-1</numToProcess>
<deployAs>immediate</deployAs>
<checkpoint batch="0" time="300000ms"/>
<timerImpl></timerImpl>
</cpeConfig>
</cpeDescription>
