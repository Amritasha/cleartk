<?xml version="1.0" encoding="UTF-8"?>
<cpeDescription xmlns="http://uima.apache.org/resourceSpecifier">
<collectionReader>
<collectionIterator>
<descriptor>
<import name="org.cleartk.corpus.ace2005.Ace2005GoldReader"/>
</descriptor>
<configurationParameterSettings>
<nameValuePair>
<name>AceCorpusDir</name>
<value>
<string>CHANGEME/optimization/English/all</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</collectionIterator>
</collectionReader>
<casProcessors casPoolSize="3" processingUnitThreadCount="1">
<casProcessor deployment="integrated" name="ACE2005Parser">
<descriptor>
<import name="org.cleartk.corpus.ace2005.Ace2005GoldAnnotator"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
</casProcessor>
<casProcessor deployment="integrated" name="Ace2005Writer">
<descriptor>
<import name="org.cleartk.corpus.ace2005.Ace2005Writer"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>OutputDirectory</name>
<value>
<string>test/data</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
<casProcessor deployment="integrated" name="XWriter">
<descriptor>
<import name="org.cleartk.util.XWriter"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>OutputDirectory</name>
<value>
<string>test/data</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
</casProcessors>
<cpeConfig>
<numToProcess>-1</numToProcess>
<deployAs>immediate</deployAs>
<checkpoint batch="0" time="300000ms"/>
<timerImpl></timerImpl>
</cpeConfig>
</cpeDescription>
