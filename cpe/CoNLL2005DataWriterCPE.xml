<?xml version="1.0" encoding="UTF-8"?>
<cpeDescription xmlns="http://uima.apache.org/resourceSpecifier">
<collectionReader>
<collectionIterator>
<descriptor>
<import name="org.cleartk.srl.conll2005.Conll2005GoldReader"/>
</descriptor>
<configurationParameterSettings>
<nameValuePair>
<name>CoNLL2005DataFile</name>
<value>
<string>data/conll 2005/conll2005st-release/train-set.gz</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</collectionIterator>
</collectionReader>
<casProcessors casPoolSize="3" processingUnitThreadCount="1">
<casProcessor deployment="integrated" name="Conll2005GoldAnnotator">
<descriptor>
<import name="org.cleartk.srl.conll2005.Conll2005GoldAnnotator"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
</casProcessor>
<casProcessor deployment="integrated" name="SnowballStemmer">
<descriptor>
<import name="org.cleartk.token.snowball.SnowballStemmer"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
</casProcessor>
<casProcessor deployment="integrated" name="ArgumentDataWriter">
<descriptor>
<import name="org.cleartk.srl.ArgumentDataWriter"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>OutputDirectory</name>
<value>
<string>test/data/srl/argument</string>
</value>
</nameValuePair>
<nameValuePair>
<name>DataWriter</name>
<value>
<string>org.cleartk.classifier.svmlight.OVASVMlightDataWriter</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
</casProcessors>
<cpeConfig>
<numToProcess>-1</numToProcess>
<deployAs>immediate</deployAs>
<checkpoint batch="0" time="300000ms"/>
<timerImpl></timerImpl>
</cpeConfig>
</cpeDescription>
