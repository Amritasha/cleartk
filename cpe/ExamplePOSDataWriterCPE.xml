<?xml version="1.0" encoding="UTF-8"?>
<cpeDescription xmlns="http://uima.apache.org/resourceSpecifier">
<collectionReader>
<collectionIterator>
<descriptor>
<import name="org.cleartk.corpus.penntreebank.PennTreebankReader"/>
</descriptor>
<configurationParameterSettings>
<nameValuePair>
<name>CorpusDirectory</name>
<value>
<string>C:/Data/treebank/wsj/mrg</string>
</value>
</nameValuePair>
<nameValuePair>
<name>Sections</name>
<value>
<string>02-03</string>
</value>
</nameValuePair>
</configurationParameterSettings>
</collectionIterator>
</collectionReader>
<casProcessors casPoolSize="3" processingUnitThreadCount="1">
<casProcessor deployment="integrated" name="TreebankGoldAnnotator">
<descriptor>
<import name="org.cleartk.syntax.treebank.TreebankGoldAnnotator"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
</casProcessor>
<casProcessor deployment="integrated" name="SnowballStemmer">
<descriptor>
<import name="org.cleartk.token.snowball.SnowballStemmer"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
</casProcessor>
<casProcessor deployment="integrated" name="ExamplePOSDataWriter">
<descriptor>
<import name="org.cleartk.example.ExamplePOSDataWriter"/>
</descriptor>
<deploymentParameters/>
<errorHandling>
<errorRateThreshold action="terminate" value="0/1000"/>
<maxConsecutiveRestarts action="terminate" value="30"/>
<timeout max="100000" default="-1"/>
</errorHandling>
<checkpoint batch="10000" time="1000ms"/>
<configurationParameterSettings>
<nameValuePair>
<name>Compress</name>
<value>
<boolean>true</boolean>
</value>
</nameValuePair>
</configurationParameterSettings>
</casProcessor>
</casProcessors>
<cpeConfig>
<numToProcess>-1</numToProcess>
<deployAs>immediate</deployAs>
<checkpoint batch="0" time="300000ms"/>
<timerImpl></timerImpl>
</cpeConfig>
</cpeDescription>
